@{
    ViewData["Title"] = "Phonedetail";
}
@{
    Layout = "_Layout3";
}
@model PhoneList
<div class="container">
    <div class="row">
        <div class="col-md-4 phoneimg">
            <div id="myCarousel" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner" role="listbox">
                    <div class="item active">
                        <img src="~/images/@Model.p.Img" alt="手机1">
                    </div>
                    <div class="item">
                        <img src="~/images/d_p2.jpg" alt="手机2">
                    </div>
                    <div class="item">
                        <img src="~/images/d_p3.jpg" alt="手机3">
                    </div>
                    <div class="item">
                        <img src="~/images/d_p4.jpg" alt="手机4">
                    </div>
                    <div class="item">
                        <img src="~/images/d_p5.jpg" alt="手机5">
                    </div>
                </div>
                <a href="#myCarousel" class="carousel-control left" data-slide="prev" role="button">
                    <span class="glyphicon glyphicon-chevron-left"></span>
                    <span class="sr-only">上一页</span>
                </a>
                <a href="#myCarousel" class="carousel-control right" data-slide="next" role="button">
                    <span class="glyphicon glyphicon-chevron-right"></span>
                    <span class="sr-only">下一页</span>
                </a>
                <ol class="ollist">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"><img src="~/images/@Model.p.Img" alt="手机1" width="54px" height="54px"></li>
                    <li data-target="#myCarousel" data-slide-to="1"><img src="~/images/d_p2.jpg" alt="手机2" width="54px" height="54px"></li>
                    <li data-target="#myCarousel" data-slide-to="2"><img src="~/images/d_p3.jpg" alt="手机3" width="54px" height="54px"></li>
                    <li data-target="#myCarousel" data-slide-to="3"><img src="~/images/d_p4.jpg" alt="手机4" width="54px" height="54px"></li>
                    <li data-target="#myCarousel" data-slide-to="4"><img src="~/images/d_p5.jpg" alt="手机5" width="54px" height="54px"></li>
                </ol>
                <div class="clear"></div>
            </div>
        </div>
        <div class="col-md-8">
            <!--标题-->
            <div class="row">
                <div class="sell text-center col-sm-offset-1 col-sm-10">
                    <h3 id="title">@Model.p.Name</h3>
                    <p class="glyphicon glyphicon-thumbs-up" id="introduction">
                        @Model.p.Introduce
                    </p>
                </div>
            </div>
            <!--价格-->
            <div class="row">
                <div id="price" class=" col-sm-offset-1 col-sm-10 bg-info">
                    <div id="price3">
                        <span id="price1">
                            <span id="head">线上价格</span>
                            <span>￥</span>
                            <span id="price2">@Model.p.Price</span>
                        </span><a class="jjtz" asp-page="/">降价通知</a>
                    </div>
                    <div>
                        <span id="head">促销</span>
                        <button id="btn" class="btn btn-primary btn-sm">高端蓝牙耳机</button>
                        <button id="btn" class="btn btn-primary btn-sm">高级车充</button>
                        <button id="btn" class="btn btn-primary btn-sm">小米10000毫安充电宝</button>
                    </div>
                </div>
            </div>
            <div class="row">
                <!--分类选择-->
                <div id="option" class="col-sm-10  col-sm-offset-1">
                    <!--颜色-->
                    <div id="color" class="chioce">
                        <span id="head">选择颜色</span>
                        <button id="btn" class="btn btn-default color">黑色</button>
                        <button class="btn btn-default">白色</button>
                    </div>
                    <!--版本-->
                    <div id="version" class="chioce">
                        <span id="head">选择版本</span>
                        <button id="btn" class="btn btn-default">6GB 64GB</button>
                        <button class="btn btn-default">6GB 128GB</button>
                        <button id="btn" class="btn btn-default">8GB 128GB全陶瓷版</button>
                    </div>
                    <!--购买方式-->
                    <div id="method" class="chioce">
                        <span id="head">购买方式</span>
                        <button id="btn" class="btn btn-default">裸机</button>
                        <button class="btn btn-default">套装</button>
                    </div>
                    <!--购买渠道-->
                    <div id="way" class="chioce">
                        <span id="head">购买渠道</span>
                        <button class="btn btn-default">官方标配</button>
                        <button class="btn btn-default">移动优惠购</button>
                        <button class="btn btn-default">联通优惠购</button>
                        <button class="btn btn-default">电信优惠购</button>
                    </div>
                    <!--增值保障-->
                    <div id="way" class="chioce">
                        <span id="head">增值保障</span>
                        <div class="btn-group">
                            <button type="button" class="btn btn-default">全保修2年￥199</button>
                            <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
                                <span class="caret"></span>
                                <span class="sr-only">切换下拉菜单</span>
                            </button>
                            <ul class="dropdown-menu" role="menu">
                                <li><a asp-page="/">全保修2年￥199</a></li>
                                <li><a asp-page="/">全保修3年￥259 </a></li>
                                <li><a asp-page="/">自营全保修￥229</a></li>
                            </ul>
                        </div>
                        <div class="btn-group">
                            <button type="button" class="btn btn-default">自营碎屏保￥129</button>
                            <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
                                <span class="caret"></span>
                                <span class="sr-only">切换下拉菜单</span>
                            </button>
                            <ul class="dropdown-menu" role="menu">
                                <li><a asp-page="/">自营碎屏保￥129</a></li>
                                <li><a asp-page="/">屏碎保一年￥99</a></li>
                                <li><a asp-page="/">屏碎保两年￥149</a></li>
                                <li><a asp-page="/">意外保两年￥159</a></li>
                            </ul>
                        </div>
                        <div class="btn-group">
                            <button type="button" class="btn btn-default">自营延长保￥129</button>
                            <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
                                <span class="caret"></span>
                                <span class="sr-only">切换下拉菜单</span>
                            </button>
                            <ul class="dropdown-menu" role="menu">
                                <li><a asp-page="/">自营延长保￥129</a></li>
                                <li><a asp-page="/">换新保两年￥259</a></li>
                                <li><a asp-page="/">电池保两年￥129</a></li>
                            </ul>
                        </div>
                    </div>
                    <!--白条分期-->
                    <div id="fq" class="chioce">
                        <span id="head">白条分期</span>
                        <button id="fq" class="btn btn-default">不分期</button>
                        <button class="btn btn-default">￥1079.13起*3期</button>
                        <button class="btn btn-default">￥545.96起*6期</button>
                        <button class="btn btn-default">￥2749.38起*12期</button>
                    </div>
                    <!--购物车-->
                    <div id="gwc" class="chioce">
                        <span id="head">数量</span>&nbsp; &nbsp; &nbsp; &nbsp;
                        <input type="number" class="number" value="1" min="1" max="999">
                        <a class="btn btn-danger btn-lg" href="/Cart/AddCart/@Model.p.ObjId?retUrl=@ViewBag.contBuy" role="button">加入购物车</a>
                        <a class="btn btn-danger btn-lg" href="/Cart/AddFavi/@Model.p.ObjId?retUrl=@ViewBag.contBuy" role="button">收藏</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
